pipeline {
    agent any

    stages {
        stage('Number Range') {
            steps {
                script {
                    sh '''
                    echo "Enter the starting value of the range:"
                    read a
                    echo "Enter the ending value of the range:"
                    read b
                    while [ $a -le $b ]; do
                        echo $a
                        a=$((a+1))
                    done
                    '''
                }
            }
        }
    }
}
