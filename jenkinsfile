pipeline {
    agent any

    parameters {
        string(name: 'START', defaultValue: '1', description: 'Enter the starting value of the range')
        string(name: 'END', defaultValue: '10', description: 'Enter the ending value of the range')
    }

    stages {
        stage('Number Range') {
            steps {
                script {
                    def start = params.START.toInteger()
                    def end = params.END.toInteger()

                    for (int i = start; i <= end; i++) {
                        echo "$i"
                    }
                }
            }
        }
    }
}
